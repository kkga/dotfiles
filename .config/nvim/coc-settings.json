{
  "diagnostic": {
    "errorSign": "> ",
    "warningSign": "- ",
    "displayByAle": true
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    },
    "zls": {
      "command": "/home/kkga/.local/bin/zls",
      "filetypes": ["zig"]
    },
    "lua": {
      "command": "/bin/lua-lsp",
      "filetypes": ["lua"]
    }
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "coc.preferences.extensionUpdateCheck": "never"
}
