#!/bin/sh
exec j4-dmenu-desktop \
	--usage-log="$HOME/.cache/j4" \
	--no-generic \
	--display-binary \
	--use-xdg-de \
	--term="foot" \
	--dmenu="(cat ; (stest -flx $(echo "$PATH" |
		tr : ' ') |
		sort -u)) |
		fzf-menu --prompt 'RUN> ' --bind=tab:down,btab:up --preview 'whatis {}' --preview-window right:50%:border-left"
